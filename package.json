{
  "name": "next-boilerplate",
  "version": "1.0.0",
  "description": "An opinionated Next.js boilerplate",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaveBitter/next-boilerplate.git"
  },
  "keywords": [
    "nextjs",
    "next",
    "react",
    "reactjs",
    "boilerplate",
    "stylelint",
    "eslint",
    "enzyme",
    "jest",
    "scss",
    "storybook",
    "husky",
    "boilerplate",
    "typescript"
  ],
  "author": "Dave Bitter <daveybitter@gmail.com> (https://www.davebitter.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaveBitter/next-boilerplate/issues"
  },
  "homepage": "https://github.com/DaveBitter/next-boilerplate#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "storybook": "start-storybook -p 1337",
    "storybook:build": "build-storybook -c .storybook -o .storybook-build",
    "test": "jest --config config/tests/jest.config.js --verbose",
    "test:watch": "jest --config config/tests/jest.config.js --watch --verbose",
    "test:coverage": "jest --config config/tests/jest.config.js --coverage --verbose",
    "eslint": "eslint --fix",
    "stylelint": "stylelint **/*.scss --syntax=scss"
  },
  "lint-staged": {
    "*.js": "yarn eslint",
    "*.scss": "yarn stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "next": "^10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.2.9",
    "@svgr/webpack": "^5.5.0",
    "@types/enzyme": "^3.10.8",
    "@types/highlight.js": "^9.12.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^5.2.4",
    "enzyme": "^3.11.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "json-loader": "^0.5.7",
    "lint-staged": "^11.0.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "postcss": "^8.2.15",
    "prop-types": "^15.7.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "sass-loader": "^11.1.1",
    "source-map-loader": "^2.0.2",
    "storybook": "^6.2.5",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-strict-value": "^1.7.12",
    "stylelint-scss": "^3.19.0",
    "svg-react-loader": "^0.4.6",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.3.0",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1"
  }
}